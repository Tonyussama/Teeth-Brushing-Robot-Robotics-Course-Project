}
//1 init 20
//2 init 45
//3 init 0
//4 init 90
void loop() {
  servo1.write(20);
  delay(1000);
  servo2.write(45);
  delay(1000);
  servo3.write(0);
  delay(1000);
  servo4.write(90);
  // Move back to initial positions
  for(int i = 1; i< 48; i++)
  {
    servo1.write(q0[i]+180);
    servo2.write(q1[i]);
    servo3.write(q2[i]);
    servo4.write(q3[i]-60);
